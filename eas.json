{
    "cli": {
        "version": ">= 9.2.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development"
        },
        "ios-simulator": {
            "extends": "development",
            "ios": {
                "simulator": true
            },
            "channel": "ios-simulator"
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview"
        },
        "production": {
            "android": {
                "image": "latest"
            },
            "ios": {
                "image": "latest"
            },
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
